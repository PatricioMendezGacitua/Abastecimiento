<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core" xmlns:l="sap.ui.layout">
	<Dialog busy='{editInforme>/busy}' busyIndicatorDelay='0' title="Ingresar Posiciones" type='Message' contentWidth="400px">
		<content>
			<VBox class="sapUiTinyMargin">
				<l:Grid hSpacing="0.5" vSpacing="0.5" width="100%" class="sapUiTinyMarginTop">
					<VBox>
						<Label class="font-xSmaller" text="Código Material:" required="true"></Label>
						<Input id="idPosTraspasoCodMatIngresar" class="fontBold" width="100%"></Input>
						<layoutData>
							<l:GridData span="XL4 L4 M6 S6"/>
						</layoutData>
					</VBox>
					<VBox>
						<!--	<Label class="font-xSmaller" text="Posición:" visible="false" width="100%" required="true"></Label>
									<Input id="idPosTraspasoPosicionIngresar" visible="false" class="fontBold" width="100%" type="Number"></Input>
									<layoutData>
										<l:GridData span="XL4 L4 M6 S6"/>
									</layoutData>
									-->
						<Label ></Label>
						<Button id="idPosTraspasoPosicionIngresar" type="Emphasized" text="Buscar" icon="sap-icon://search" iconFirst="true" press="onBuscarFrag"></Button>
						<layoutData>
							<l:GridData span="XL4 L4 M6 S6"/>
						</layoutData>
					</VBox>
				</l:Grid>
				<l:Grid hSpacing="0.5" vSpacing="0.5" width="100%" class="sapUiTinyMarginTop" id="idVboxFrag" visible="false">
					<VBox>
						<Label class="font-xSmaller" text="Denominación Material:" width="100%" required="true"></Label>
						<Text id="idPosTraspasoDenMatIngresar" class="fontBold" width="100%"></Text>
						<layoutData>
							<l:GridData span="XL12 L12 M12 S12"/>
						</layoutData>
					</VBox>
					<VBox>
						<Label class="font-xSmaller" text="Ubicación:" width="100%" required="true"></Label>
						<VBox justifyContent="Center">
							<Text id="idPosTraspasoUbicacionIngresar" class="fontBold" width="100%"></Text>
						</VBox>
						<layoutData>
							<l:GridData span="XL4 L4 M4 S4"/>
						</layoutData>
					</VBox>
					<VBox>
						<Label class="font-xSmaller" text="Almacén:" width="100%" required="true"></Label>
						<Input id="idPosTraspasoAlmacenIngresar" valueHelpOnly="true" showValueHelp="true" valueHelpRequest=".openListAlmacenesCompleta"
							class="fontBold" width="100%"></Input>
						<layoutData>
							<l:GridData span="XL5 L5 M4 S4"/>
						</layoutData>
					</VBox>
					<VBox>
						<Label class="font-xSmaller" text="Und. Medida:" width="100%" required="true"></Label>
						<Text id="idPosTraspasoUnMedidaIngresar" class="fontBold" width="100%"></Text>
						<layoutData>
							<l:GridData span="XL4 L4 M4 S4"/>
						</layoutData>
					</VBox>
				</l:Grid>
			</VBox>
		</content>
		<beginButton>
			<Button id="btIngresarPosTraspaso" type="Emphasized" text="Ingresar" press="onInsertarPosTraspasoAdd"/>
		</beginButton>
		<endButton>
			<Button text="Cancelar" press="onInsertarPosTraspasoClose"/>
		</endButton>
	</Dialog>
</core:FragmentDefinition>